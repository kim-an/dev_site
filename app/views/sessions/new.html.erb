<h1>Log In</h1>
<p id='alert'><%= alert %></p>
<%= form_tag sessions_path do %>
  <div class="field">
    <%= label_tag :business_name %>
    <%= text_field_tag :business_name, params[:business_name] %>
  </div

  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>

  <div class="action"><%= submit_tag "Log In" %></div>

  <% end %>
